{
    "files.associations": {
        "*.css": "tailwindcss",
        "*.scss": "tailwindcss",
    },
    "editor.quickSuggestions": {
        "strings": true
    },
    "tailwindCSS.experimental.classRegex": [
        [
            "ui:\\s*{([^)]*)\\s*}",
            "[\"'`]([^\"'`]*).*?[\"'`]"
        ],
        [
            "/\\*ui\\*/\\s*{([^;]*)}",
            ":\\s*[\"'`]([^\"'`]*).*?[\"'`]"
        ]
    ],
    "tailwindCSS.classAttributes": [
        "class",
        "className",
        "ngClass",
        "ui"
    ]
}